<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_fragment_search"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#ffffffff"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="3dp"

            android:src="@drawable/home_search"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"

            android:textSize="14sp"
            android:text="다른 부모들은 어떤 분유를 검색했을까?"
            android:textStyle="bold"
            android:textColor="#57C670" />

        <ImageView
            android:id="@+id/othermom"
            android:layout_width="24dp"
            android:layout_height="12dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="30dp"

            android:src="@drawable/home_more"  />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#EEEEEE" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="139dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/othermomlayout1"
            android:layout_weight="1">

            <ImageView
                android:layout_width="80dp"
                android:layout_height="100dp"
                android:src="@drawable/home_drymilk_1"
                android:layout_gravity="center"
                android:id="@+id/Search_Drymilk_IV"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/home_first"
                    android:id="@+id/Search_Number_IV"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="9sp"
                        android:text="분유회사"
                        android:textColor="#95989A"
                        android:id="@+id/Search_Company_TV"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="11sp"
                        android:text="분유A"
                        android:textColor="#67C570"
                        android:id="@+id/Search_Drymilk_TV1"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            android:id="@+id/othermomlayout2"
            android:layout_weight="1">


            <ImageView
                android:layout_width="80dp"
                android:layout_height="100dp"
                android:src="@drawable/home_drymilk_2"
                android:layout_gravity="center"
                android:id="@+id/Search_Drymilk_IV2"/>
                />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="4dp"
                    android:src="@drawable/home_second" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="9sp"
                        android:text="분유회사"
                        android:textColor="#95989A"
                        android:id="@+id/Search_Company_TV2"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="11sp"
                        android:text="분유A"
                        android:textColor="#67C570"
                        android:id="@+id/Search_Drymilk_TV2"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            android:id="@+id/othermomlayout3"
            android:layout_weight="1">

            <ImageView
                android:layout_width="80dp"
                android:layout_height="100dp"
                android:src="@drawable/home_drymilk_3"
                android:layout_gravity="center"
                android:id="@+id/Search_Drymilk_IV3"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@drawable/home_third" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="9sp"
                        android:text="분유회사"
                        android:textColor="#95989A"
                        android:id="@+id/Search_Company_TV3"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:textSize="11sp"
                        android:text="분유A"
                        android:textColor="#67C570"
                        android:id="@+id/Search_Drymilk_TV3"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#EEEEEE" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            android:layout_marginLeft="13dp"

            android:src="@drawable/home_review"
            android:layout_gravity="center_vertical"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="4dp"

            android:textSize="14sp"
            android:text="리뷰가 많은 분유"
            android:textStyle="bold"
            android:textColor="#57C670"/>

        <ImageView
            android:id="@+id/manyreview"
            android:layout_width="24dp"
            android:layout_height="12dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="170dp"

            android:src="@drawable/home_more"  />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#EAEAEA" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginLeft="22dp"
            android:id="@+id/manyreviewlayout1"
            android:weightSum="10">

            <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_gravity="center_vertical"

                android:src="@drawable/home_first"
                android:id="@+id/Review_Number_IV1" />

            <ImageView
                android:layout_width="45dp"
                android:layout_height="wrap_content"

                android:src="@drawable/drymilk_1_75dp"
                android:id="@+id/home_drymilk_1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="분유D"
                android:textColor="#67C570"
                android:textSize="11sp"

                android:layout_gravity="center_vertical"
                android:id="@+id/Review_Drymilk_TV1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"

                android:text="분유설명"
                android:textColor="#C0C0C0"
                android:textSize="9sp"

                android:layout_centerVertical="true"
                android:id="@+id/Review_Content_TV1"
                android:layout_gravity="center_vertical"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEAEA" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginLeft="22dp"
            android:id="@+id/manyreviewlayout2"
            android:weightSum="10">

            <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_gravity="center_vertical"

                android:src="@drawable/home_second"
                android:id="@+id/Review_Number_IV2" />

            <ImageView
                android:layout_width="45dp"
                android:layout_height="wrap_content"

                android:src="@drawable/drymilk_2_75dp"
                android:id="@+id/home_drymilk_2"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="분유D"
                android:textColor="#67C570"
                android:textSize="11sp"

                android:layout_gravity="center_vertical"
                android:id="@+id/Review_Drymilk_TV2"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"

                android:text="분유설명"
                android:textColor="#C0C0C0"
                android:textSize="9sp"

                android:layout_centerVertical="true"
                android:id="@+id/Review_Content_TV2"
                android:layout_gravity="center_vertical"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#EAEAEA" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginLeft="22dp"
            android:id="@+id/manyreviewlayout3"
            android:weightSum="10">

            <ImageView
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_gravity="center_vertical"

                android:src="@drawable/home_third"
                android:id="@+id/Review_Number_IV3" />

            <ImageView
                android:layout_width="45dp"
                android:layout_height="wrap_content"

                android:src="@drawable/drymilk_3_75dp"
                android:id="@+id/home_drymilk_3"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textSize="11sp"
                android:text="분유D"
                android:textColor="#67C570"

                android:layout_gravity="center_vertical"
                android:id="@+id/Review_Drymilk_TV3"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"

                android:text="분유설명"
                android:textColor="#C0C0C0"
                android:textSize="9sp"

                android:layout_centerVertical="true"
                android:id="@+id/Review_Content_TV3"
                android:layout_gravity="center_vertical"/>

        </LinearLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#EAEAEA" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"

        android:gravity="center_vertical">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"

            android:src="@drawable/home_ingredient"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"

            android:textSize="14sp"
            android:textStyle="bold"
            android:textColor="#67C570"

            android:text="오늘의 성분"
            android:id="@+id/textView6" />

        <!--<LinearLayout-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content">-->

            <!--<ImageView-->
                <!--android:id="@+id/moretoday_ingre"-->
                <!--android:layout_width="24dp"-->
                <!--android:layout_height="12dp"-->
                <!--android:layout_gravity="center_vertical"-->
                <!--android:layout_marginLeft="200dp"-->

                <!--android:src="@drawable/home_more"  />-->

        <!--</LinearLayout>-->

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#EAEAEA" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:layout_marginRight="9dp"
            android:layout_marginTop="5dp"

            android:src="@drawable/signup_line"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"

            android:textSize="14sp"
            android:textStyle="bold"
            android:textColor="#67C570"
            android:text="비타민 B1"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"

            android:layout_marginLeft="33dp"

            android:textColor="#67C570"
            android:textSize="11sp"
            android:text="비타민의 한 종류로 티아민이라고도 불린다.\n곡류, 콩, 견과류, 등에 함유되어 있다."/>

    </LinearLayout>



</LinearLayout>