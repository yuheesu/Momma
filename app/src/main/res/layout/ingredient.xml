<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#ffffffff"
    >
    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
    <TextView
        android:id="@+id/searchTV"
        android:layout_marginTop="5dp"
        android:layout_gravity="center_horizontal"
        android:layout_width="274dp"
        android:layout_height="40dp"
        android:padding="16dp"
        android:drawableLeft="@drawable/ingredient_search"
        android:drawablePadding="7dp"
        android:textCursorDrawable="@null"
        android:background="@drawable/border"
        android:singleLine="true">
    </TextView>

        <Spinner
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/ingredientsp"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp"
            android:layout_centerVertical="true"

            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"></Spinner>

        <TextView
            android:layout_marginTop="3dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ingredient_updown"
            android:layout_alignTop="@+id/searchTV"
            android:layout_alignRight="@+id/ingredientsp"
            android:layout_alignEnd="@+id/ingredientsp"
            android:id="@+id/textView2" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:gravity="center"
        android:layout_height="70dp">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:paddingRight="8dp"

                android:id="@+id/overview"
        android:text="텍스트"
        android:textColor="#65C570"/>
    </LinearLayout>
    <ExpandableListView
        android:id="@+id/expandablelist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:groupIndicator="@null"
        android:divider="#EAEAEA"
        android:dividerHeight="2dp">
    </ExpandableListView>
    <View
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:background="#EAEAEA" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/ImageView01"
            android:src="@drawable/ingredient_grade"
            android:layout_height="19dp"
            android:layout_width="19dp"
            android:layout_marginLeft="20dip"></ImageView>

        <TextView
            android:id="@+id/tvGroup"
            android:layout_width="280dp"
            android:layout_height="45dip"
            android:text="성분포함 순위"
            android:gravity="center_vertical|left"
            android:paddingLeft="5dip"
            android:textColor="#67C570"
            android:textStyle="bold"
            android:textSize="17dip"></TextView>

        <Button
            android:id="@+id/IngredientLIst_BT"
            android:gravity="center_vertical|right"
            android:background="@drawable/ingredient_add"
            android:layout_height="24dp"
            android:layout_width="24dp">
        </Button>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#EAEAEA" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/Ingredientmilks"
        android:visibility="invisible"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Ingredientmilk01"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:id="@+id/ingretabrank1_IV"
                android:background="@drawable/drymilk_1_75dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank1_TV1"
                android:layout_gravity="center_horizontal"
           />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank1_TV2"
                android:layout_gravity="center_horizontal"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Ingredientmilk02"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:id="@+id/ingretabrank2_IV"
                android:background="@drawable/drymilk_2_75dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank2_TV1"
                android:layout_gravity="center_horizontal"
    />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank2_TV2"
                android:layout_gravity="center_horizontal"
             />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Ingredientmilk03"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:id="@+id/ingretabrank3_IV"
                android:background="@drawable/drymilk_3_75dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank3_TV1"
                android:layout_gravity="center_horizontal"
             />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank3_TV2"
                android:layout_gravity="center_horizontal"
            />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Ingredientmilk04"
            android:layout_weight="1"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:id="@+id/ingretabrank4_IV"
                android:background="@drawable/drymilk_1_75dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank4_TV1"
                android:layout_gravity="center_horizontal"
         />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ingretabrank4_TV2"
                android:layout_gravity="center_horizontal"
             />
        </LinearLayout>

    </LinearLayout>


</LinearLayout>
